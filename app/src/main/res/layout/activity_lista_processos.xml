<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/degrade_radial"
    tools:context="com.example.diego.andamentoprocessualtjmg.ACTIVITYS.ListaProcessosActivity">

    <TextView
        android:id="@+id/tvVara"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="TextView"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="109dp"
        android:layout_below="@+id/tvNumero"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp" />

    <TextView
        android:id="@+id/tvNumero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tvVara"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/tvVara"
        android:layout_marginTop="25dp"
        android:text="TextView"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/tvStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="52dp"
        android:layout_marginRight="52dp"
        android:text="TextView"
        android:textSize="18sp"
        android:layout_alignBaseline="@+id/tvVara"
        android:layout_alignBottom="@+id/tvVara"
        android:layout_alignRight="@+id/tvLinha1"
        android:layout_alignEnd="@+id/tvLinha1" />

    <TextView
        android:id="@+id/tvLinha1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:text="__________________________________________________________"
        android:layout_below="@+id/tvVara"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/tvlinha3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:text="__________________________________________________________"
        android:layout_below="@+id/tvAssunto"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/tvClasse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_below="@+id/tvLinha1"
        android:layout_alignLeft="@+id/tvVara"
        android:layout_alignStart="@+id/tvVara" />

    <TextView
        android:id="@+id/tvAssunto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_below="@+id/tvClasse"
        android:layout_alignLeft="@+id/tvClasse"
        android:layout_alignStart="@+id/tvClasse" />

    <TextView
        android:id="@+id/tvCS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_above="@+id/tvAssunto"
        android:layout_alignLeft="@+id/tvStatus"
        android:layout_alignStart="@+id/tvStatus" />

    <TextView
        android:id="@+id/tvPartes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/tvlinha3"
        android:layout_alignLeft="@+id/tvAssunto"
        android:layout_alignRight="@+id/tvlinha3"
        android:layout_alignStart="@+id/tvAssunto"
        android:layout_below="@+id/tvlinha3"
        android:layout_marginTop="11dp"
        android:text="TextView"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Última(s) Movimentação(ões): "
        android:layout_marginTop="12dp"
        android:layout_below="@+id/tvPartes"
        android:layout_alignLeft="@+id/tvPartes"
        android:layout_alignStart="@+id/tvPartes" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/textView18">

        <TextView
            android:id="@+id/tvMovimento"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="11sp" />
    </ScrollView>

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="?attr/actionModeShareDrawable"
        android:layout_alignBottom="@+id/tvNumero"
        android:layout_alignRight="@+id/tvLinha1"
        android:layout_alignEnd="@+id/tvLinha1" />

</RelativeLayout>

